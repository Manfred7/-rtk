{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "src/templates/component/TemplateName.js",
        "style": "src/templates/component/TemplateName.style.css"
      },
      "path": "src/Components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "src/templates/page/TemplateName.js",
        "style": "src/templates/page/TemplateName.style.css"
      },
      "path": "src/Pages",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}
